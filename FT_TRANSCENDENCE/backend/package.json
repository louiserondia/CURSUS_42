{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "docker:start": "sudo service docker start",
    "prisma:dev:deploy": "prisma migrate deploy && prisma generate",
    "db:dev:rm": "docker-compose down",
    "db:dev:up": "docker-compose up -d dev-db",
    "db:dev:restart": "npm run db:dev:rm && npm run db:dev:up",
    "prisma:test:deploy": "dotenv -e .env.test -- prisma migrate deploy && prisma generate",
    "db:test:up": "docker-compose up -d test-db",
    "db:test:restart": "npm run db:dev:rm && npm run db:test:up",
    "prisma:test:studio": "dotenv -e .env.test -- prisma studio",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "pretest:e2e": "npm run db:test:restart",
    "test:e2e": "dotenv -e .env.test -- jest --watch --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/axios": "^2.0.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/event-emitter": "^1.4.1",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/platform-socket.io": "^9.4.1",
    "@nestjs/schedule": "^2.2.2",
    "@nestjs/websockets": "^9.4.1",
    "@prisma/client": "^4.15.0",
    "@studimax/elo": "^1.0.1",
    "@types/passport-jwt": "^3.0.8",
    "argon2": "^0.30.3",
    "axios": "^1.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cli": "^1.0.1",
    "date-fns": "^2.30.0",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.2.1",
    "express-session": "^1.17.3",
    "luxon": "^3.3.0",
    "multer": "^1.4.5-lts.1",
    "mute": "^2.0.6",
    "nest": "^0.1.6",
    "pactum": "^3.4.0",
    "passport": "^0.6.0",
    "passport-42": "^1.2.6",
    "passport-auth0": "^1.4.3",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prisma-client": "^0.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
    "speakeasy": "^2.0.0",
    "studio": "^0.13.5",
    "uuid": "^9.0.0",
    "websocket": "^1.0.34"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "29.2.4",
    "@types/multer": "^1.4.7",
    "@types/node": "18.11.18",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.3.1",
    "prettier": "^2.3.2",
    "prisma": "^4.16.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
